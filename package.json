{
  "name": "@salzpate/react-ui",
  "description": "React UI component library built with Tailwind CSS",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "version": "0.1.11",
  "license": "Apache-2.0",
  "author": "Ronny Krammer",
  "repository": {
    "type": "git",
    "url": "https://github.com/salzpate/react-ui.git"
  },
  "module": "dist/react-ui.esm.js",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "start": "tsdx watch",
    "build-storybook": "build-storybook",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "build": "webpack --mode=production --node-env=production",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "watch": "webpack --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "size-limit": [
    {
      "path": "dist/react-ui.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-ui.esm.js",
      "limit": "10 KB"
    }
  ],
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  },
  "peerDependencies": {
    "classnames": "^2.3.1",
    "next": "^12.0.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.20.5"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@size-limit/preset-small-lib": "^7.0.3",
    "@storybook/addon-actions": "^6.4.7",
    "@storybook/addon-essentials": "^6.4.7",
    "@storybook/addon-links": "^6.4.7",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.4.7",
    "@storybook/manager-webpack5": "^6.4.7",
    "@storybook/react": "^6.4.7",
    "@testing-library/jest-dom": "^5.16.0",
    "@testing-library/react": "^12.1.2",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/styled-components": "^5.1.16",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@webpack-cli/generators": "^2.4.1",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.5.1",
    "eslint": "^8.4.0",
    "eslint-config-next": "^12.0.6",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "mini-css-extract-plugin": "^1.5.0",
    "next": "^12.0.6",
    "postcss": "^8.4.4",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.0.1",
    "react-is": "^17.0.2",
    "size-limit": "^7.0.3",
    "storybook-addon-next-router": "^3.1.0",
    "style-loader": "^3.3.1",
    "styled-components": "^5.3.3",
    "stylelint": "^14.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^6.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "tailwindcss": "^2.2.19",
    "ts-loader": "^8.2.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.2",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1"
  }
}
