{
  "name": "@salzpate/react-ui",
  "description": "React UI component library built with Tailwind CSS",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "version": "0.1.14",
  "license": "Apache-2.0",
  "author": "Ronny Krammer",
  "repository": {
    "type": "git",
    "url": "https://github.com/salzpate/react-ui.git"
  },
  "module": "dist/index.js",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build-storybook": "build-storybook",
    "build": "npm run esbuild",
    "esbuild": "rm -rf dist && NODE_ENV=production node ./esbuild.mjs && tsc --emitDeclarationOnly --outDir dist",
    "lint": "eslint --ext tsx --ext ts src && tsc --noEmit",
    "size": "size-limit",
    "test": "jest --config=jest.config.js",
    "storybook": "start-storybook -p 6006"
  },
  "husky": {
    "hooks": {
      "pre-commit": "eslint --ext tsx --ext ts src && tsc --noEmit"
    }
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "100 KB"
    }
  ],
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  },
  "peerDependencies": {
    "classnames": "^2.3.1",
    "next": "^12.0.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.25.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.10",
    "@size-limit/preset-big-lib": "^7.0.5",
    "@storybook/addon-actions": "^6.4.14",
    "@storybook/addon-essentials": "^6.4.14",
    "@storybook/addon-links": "^6.4.14",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.4.14",
    "@storybook/manager-webpack5": "^6.4.14",
    "@storybook/react": "^6.4.14",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/styled-components": "^5.1.21",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "@webpack-cli/generators": "^2.4.1",
    "autoprefixer": "^10.4.2",
    "esbuild": "^0.14.12",
    "esbuild-plugin-svgr": "^1.0.0",
    "eslint": "^8.7.0",
    "eslint-config-next": "^12.0.8",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^25.7.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.4.7",
    "next": "^12.0.9",
    "postcss": "^8.4.5",
    "postcss-preset-env": "^7.2.3",
    "prettier": "^2.5.1",
    "react-is": "^17.0.2",
    "size-limit": "^7.0.5",
    "storybook-addon-next-router": "^3.1.1",
    "style-loader": "^3.3.1",
    "styled-components": "^5.3.3",
    "stylelint": "^14.2.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^6.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "tailwindcss": "^3.0.15",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  }
}
